._huato_childbtnmenu
  border: 1px solid #90C8F8
._huato_menuparsbtn
  background: #ffffff !important
  color: #333333 !important
  outline: none
  ul
    background: #F8F7F7 !important
    color: #666 !important
    span
      background: #F8F7F7 !important
      color: #666 !important
    li
      height: 51px
      line-height: 51px
      font-size: 16px
      span
        background: #F8F7F7 !important
        color: #666 !important
._huato_menuparsbtn:hover
  background: #ffffff !important
  color: #333 !important
::ng-deep ._huato_menuparsbtn .ant-menu-submenu-title
  border: 1px solid #90C8F8
  border-bottom: none
  height: 60px !important
  line-height: 60px !important
  font-size: 18px !important
::ng-deep .ant-menu-dark .ant-menu-submenu-title:hover
  background: none !important
  color: #333 !important
._huato_menuparsbtn.ant-menu-submenu-open
  background: #5BADF2 !important
  color: #fff !important
._huato_menuparsbtn.ant-menu-item-selected
  background: #5BADF2 !important
  color: #fff !important
._huato_menuparsbtn.ant-menu-submenu-open:hover
  color: #fff !important
  ::ng-deep .ant-menu-submenu-title
    color: #fff !important
._huato_menuparsbtn:last-child
  ::ng-deep .ant-menu-submenu-title
    border-bottom: 1px solid #90C8F8
._huato_menuparsbtn:first-child
  ::ng-deep .ant-menu-submenu-title
    border-top: 1px solid #90C8F8
::ng-deep li.ant-menu-item.ng-star-inserted
  height: 60px
  line-height: 60px
  font-size: 18px
  background-color: white
  color: #555
  border-top: 1px solid #90c8f8 !important
  border-left: 1px solid #90c8f8 !important
  border-right: 1px solid #90c8f8 !important
  i
    opacity: 0
::ng-deep ul._huato_menulastBtncolor.ant-menu.ant-menu-root.ant-menu-dark.ant-menu-inline
  border-bottom: 1px solid #90c8f8
::ng-deep .ant-menu-dark, .ant-menu-dark .ant-menu-sub
  background: azure !important
  color: #555 !important
::ng-deep .ant-menu-dark .ant-menu-item:hover
  color: #90c8f8 !important
::ng-deep li.ant-menu-item.ng-star-inserted.ant-menu-item-selected
  background: #90c8f8 !important
  color: #fff !important
  outline: none !important
  i
    opacity: 1
::ng-deep li.ant-menu-item.ng-star-inserted:focus
  outline: none !important